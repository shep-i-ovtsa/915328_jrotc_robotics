{
	"env": {
		"vex_compilerPath": "${config:vexrobotics.vexcode.Cpp.Toolchain.Path}/clang/bin/clang",
		"vex_sdkPath": "${config:vexrobotics.vexcode.Cpp.Sdk.Home}/V5/V5_20240802_15_00_00/vexv5",
		"vex_gcc": "${vex_sdkPath}/gcc/include/c++/4.9.3",
		"vex_sdkIncludes": [
			"${vex_sdkPath}/clang/8.0.0/include/**",
			"${vex_gcc}/**",
			"${vex_gcc}/arm-none-eabi/thumb",
			"${vex_sdkPath}/gcc/include",
			"${vex_sdkPath}/include/**",
			"${workspaceFolder}/include/**",
			"${workspaceFolder}/src/**"
		]
	},
	"configurations": [
		{
			"name": "linux-gcc-x64",
			"includePath": [
				"/home/shep/Documents/robotics/VEX-V5RC-competition-template-with-PID-master/**"
			],
			"compilerPath": "/sbin/gcc",
			"intelliSenseMode": "linux-gcc-x86",
			"compilerPathInCppPropertiesJson": "/sbin/gcc",
			"mergeConfigurations": false,
			"recursiveIncludes": {},
			"browse": {
				"limitSymbolsToIncludedHeaders": true
			},
			"cppStandard": "c++17",
			"cStandard": "c17"
		},
		{
			"name": "Mac",
			"includePath": [
				"${vex_sdkIncludes}"
			],
			"compilerPath": "${vex_compilerPath}",
			"cStandard": "gnu99",
			"cppStandard": "gnu++11",
			"intelliSenseMode": "macos-clang-x86"
		},
		{
			"name": "Linux",
			"includePath": [
				"${vex_sdkIncludes}"
			],
			"compilerPath": "",
			"cStandard": "gnu99",
			"cppStandard": "gnu++11",
			"intelliSenseMode": "linux-gcc-x86"
		},
		{
			"name": "linux-c++-configuration",
			"includePath": [
				"${workspaceFolder}/vex.h**"
			],
			"defines": [],
			"compilerPath": "/sbin/clang",
			"cStandard": "c17",
			"cppStandard": "c++17",
			"intelliSenseMode": "linux-clang-x64"
		},
		{
			"name": "linux-config",
			"includePath": [
				"${workspaceFolder}/**"
			],
			"defines": [],
			"compilerPath": "/usr/bin/gcc",
			"cStandard": "c23",
			"cppStandard": "c++23",
			"intelliSenseMode": "${default}"
		}
	],
	"version": 4
}